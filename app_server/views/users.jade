extends layout

block content
  
  div(style='margin-top:30px; margin-left:30px;')
    h2 Users
    p All users are shown bellow
  hr


  div(ng-controller="myController")
    ul  
      li(ng-repeat="item in Items")

  .container-fluid.gedf-wrapper(ng-controller="usersList")
          .row
            .col-md-3
              .card
                .card-body
                  .h5 Users
                ul.list-group.list-group-flush
                  li.list-group-item
                    .h6.text-muted 
                    p(ng-repeat="userName in users.name") 
                      | {{ userName }}
                    